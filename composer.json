{
    "name": "anik/cache",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "psr-6",
        "psr-cache",
        "psr-16",
        "psr-simple-cache",
        "simple-cache-implementation",
        "cache-contract-implementation",
        "null-cache",
        "in-memory-cache",
        "runtime-cache",
        "array-cache"
    ],
    "authors": [
        {
            "name": "Syed Sirajul Islam Anik",
            "email": "sirajul.islam.anik@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anik\\Cache\\": "src/"
        },
        "files": [
            "src/methods.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Anik\\Cache\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "psr/cache": "^2.0|^3.0",
        "psr/simple-cache": "^2.0|^3.0"
    },
    "provide": {
        "psr/cache-implementation": "^2.0|^3.0",
        "psr/simple-cache-implementation": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6|^10.0"
    }
}
